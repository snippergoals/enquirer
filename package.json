{
  "name": "enquirer",
  "description": "Stylish and user-friendly prompt system that is .",
  "version": "0.0.0",
  "homepage": "https://github.com/enquirer/enquirer",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "enquirer/enquirer",
  "bugs": {
    "url": "https://github.com/enquirer/enquirer/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "mocha",
    "cover": "nyc --reporter=text --reporter=html mocha"
  },
  "dependencies": {
    "ansi-colors": "^3.1.0"
  },
  "devDependencies": {
    "yosay": "^2.0.2",
    "inquirer": "^6.2.0",
    "prompts": "^1.1.1",
    "mocha": "^5.2.0",
    "gulp-format-md": "^1.0.0",
    "nyc": "^12.0.2"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "base",
    "cli",
    "command",
    "command-line",
    "enquirer",
    "Enquirer",
    "enquirerprompt",
    "input",
    "inquire",
    "inquirer",
    "interface",
    "menu",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "lintDeps": {
    "devDependencies": {
      "files": {
        "patterns": [
          "perf/*.js",
          "examples/**/*.js"
        ]
      }
    }
  },
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "data": {
      "alias": "base"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
